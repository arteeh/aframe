@inherits AEntity

<a-box sq-collider color="@Color" position="@Pos" rotation="@Rot" scale="@Scl"></a-box>

@code {

	[Parameter]
	public string Color { get; set; } = "#FF00FF";

	public ABox() {}

	public ABox(string color,
		double posX = 0, double posY = 0, double posZ = 0, double rotX = 0, double rotY = 0, double rotZ = 0, double sclX = 1, double sclY = 1, double sclZ = 1) : base(posX,posY,posZ,rotX,rotY,rotZ,sclX,sclY,sclZ)
	{
		Color = color;
	}

}
