@inherits AEntity

<a-link title="@Title" href="@Href" image="@ImageHref" position="@Pos" rotation="@Rot"></a-link>

@code {

	[Parameter]
	public string Title { get; set; } = "TODO_PORTAL_NAME";

	[Parameter]
	public string Href { get; set; } = "https://sq-community-hub.glitch.me";

	[Parameter]
	public string ImageHref { get; set; } = "https://logo.clearbit.com/sidequestvr.com";

	public ALink() {}

	public ALink(string title, string href, string imageHref,
		double posX = 0, double posY = 0, double posZ = 0, double rotX = 0, double rotY = 0, double rotZ = 0, double sclX = 1, double sclY = 1, double sclZ = 1) : base(posX,posY,posZ,rotX,rotY,rotZ,sclX,sclY,sclZ)
	{
		Title = title;
		Href = href;
		ImageHref = imageHref;
	}

}
