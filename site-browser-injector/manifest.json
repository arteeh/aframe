{
	"manifest_version": 3,
	"name": "Site Browser Injector",
	"description": "Injects the site browser into every a-scene.",
	"version": "1.0",
	"homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/borderify",
	"icons": {
	  "48": "icons/border-48.png"
	},
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["site-browser-injector.js"]
	  }
	]  
}