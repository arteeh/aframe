{
	"manifest_version": 3,
	"name": "Site Browser",
	"description": "Injects the site browser into every A-Frame scene.",
	"version": "1.0",
	"homepage_url": "https://github.com/arteeh/aframe",
	"content_scripts": [
		{
			"world": "MAIN",
			"matches": [
				"<all_urls>"
			],
			"js": [
				"site-browser.js",
				"site-browser-injector.js"
			]
		}
	]
}