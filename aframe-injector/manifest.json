{
	"manifest_version": 3,
	"name": "A-Frame Injector",
	"description": "Injects entities into every a-scene.",
	"version": "1.0",
	"homepage_url": "https://github.com/arteeh/aframe",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"webcomponents-bundle.js",
				"site-browser.js",
				"aframe-injector.js"
			]
		}
	],
	"web_accessible_resources": [
		{
			"matches": [
				"<all_urls>"
			],
			"resources": [
				"site-browser.html"
			]
		}
	]
}